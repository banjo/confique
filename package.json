        }
    },
    "files": [
        "dist/*"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "eslint --ext .js,.ts,.json . --fix",
        "prepublishOnly": "nr build",
        "release": "bumpp && npm publish",
        "start": "esno src/index.ts",
        "test": "vitest --watch=false",
        "test:dev": "vitest",
        "typecheck": "tsc --noEmit",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@antfu/ni": "^0.21.2",
        "@banjoanton/eslint-config": "^0.1.6",
